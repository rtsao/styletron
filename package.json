{
  "name": "styletron-monorepo",
  "description": "A generic server-rendering helper for css-in-js tools",
  "author": "Ryan Tsao <ryan.j.tsao@gmail.com>",
  "homepage": "https://github.com/rtsao/styletron",
  "repository": "git@github.com:rtsao/styletron.git",
  "bugs": "https://github.com/rtsao/styletron/issues",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean",
    "lint": "eslint packages/*/src/**",
    "build-docs": "jsdoc -c jsdoc.json",
    "pretest": "npm run lint && lerna run pretest",
    "test": "lerna run test --concurrency=1",
    "predeploy-docs": "rm -rf htmldocs && npm run build-docs && echo styletron.js.org > htmldocs/CNAME",
    "deploy-docs": "push-dir --dir htmldocs --branch gh-pages"
  },
  "dependencies": {},
  "devDependencies": {
    "@rtsao/docdash": "^0.4.2",
    "benchmark": "^2.1.1",
    "buble": "^0.14.2",
    "eslint": "^3.8.1",
    "inferno": "^0.7.27",
    "inferno-component": "^0.7.27",
    "inferno-create-element": "^0.7.27",
    "inferno-dom": "^0.7.27",
    "inferno-server": "^0.7.27",
    "jsdoc": "^3.4.0",
    "lerna": "2.0.0-beta.30",
    "push-dir": "^0.4.0",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "tape": "^4.6.0",
    "unitest": "^0.14.2"
  },
  "license": "MIT"
}
