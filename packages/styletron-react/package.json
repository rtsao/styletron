{
  "name": "styletron-react",
  "version": "3.0.1",
  "description": "Official React bindings for styletron",
  "author": "Ryan Tsao <ryan.j.tsao@gmail.com>",
  "homepage": "https://github.com/rtsao/styletron",
  "repository": "git@github.com:rtsao/styletron.git",
  "bugs": "https://github.com/rtsao/styletron/issues",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/index",
  "module": "./dist/index.mjs",
  "browser": {
    "./dist/index.js": "./dist/browser.es5.js",
    "./dist/index.mjs": "./dist/browser.es5.mjs"
  },
  "es2015": {
    "./dist/browser.es5.mjs": "./dist/browser.es2015.mjs"
  },
  "es2017": {
    "./dist/browser.es5.mjs": "./dist/browser.es2017.mjs",
    "./dist/browser.es2015.mjs": "./dist/browser.es2017.mjs"
  },
  "types": "./index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "cup build",
    "pretest": "cup build-tests",
    "test": "../../node_modules/.bin/unitest --browser=dist-tests/browser.js",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@types/react": "*",
    "eslint-plugin-cup": "^1.0.0",
    "prop-types": "^15.6.0",
    "styletron-client": "^3.0.1",
    "styletron-server": "^3.0.1",
    "styletron-utils": "^3.0.0"
  },
  "peerDependencies": {
    "react": "0.14.x - 16.x"
  },
  "devDependencies": {
    "@babel/preset-react": "7.0.0-beta.37",
    "create-universal-package": "3.2.2",
    "eslint-config-cup": "1.0.0",
    "eslint-config-cup-recommended": "1.0.0",
    "react": "16.2.0",
    "react-dom": "16.2.0"
  },
  "license": "MIT"
}
